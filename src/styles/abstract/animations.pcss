@keyframes rotate {
    0%{
        transform: rotate(0);
    }
    50%{
        transform: rotate(360deg);
    }
    100%{
        transform: rotate(360deg);
    }
}
@keyframes jump {
    0%{
        transform: translateY(0);
    }
    15%{
        transform: translateY(3px);
    }
    30%{
        transform: translateY(0);
    }
    45%{
        transform: translateY(3px);
    }
    60%{
        transform: translateY(0);
    }
    100%{
        transform: translateY(0);
    }
}