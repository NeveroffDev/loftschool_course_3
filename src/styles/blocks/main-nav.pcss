.main-nav{
    width: 100%;
    margin-top: 60px;
    display: flex;
    justify-content: space-between;
    align-items: center;
    &__logo{
        font-size: 36px;
        font-weight: 800;
    }
    &__list{
        display: flex;
    }
    &__item{
        &:not(:last-child){
            margin-right: 60px;
        }
    }

}