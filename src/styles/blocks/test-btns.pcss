.test-btns{

}
.test-btns__prev,
.test-btns__next{
    width: 50px;
    height: 50px;
    border: 1px solid $text-color;
    border-radius: 50%;
    background: none;
}
.test-btns__prev{
    margin-right: 30px;
}
.test-btns__icon{
    width: 10px;
    height: 10px;
    fill: $text-color;
}
.test-btns__icon--rotate{
    transform: rotate(180deg);
}